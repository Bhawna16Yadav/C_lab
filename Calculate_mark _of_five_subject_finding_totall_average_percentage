#include <stdio.h>
int main(){
    float m1,m2,m3,m4,m5,total,average,percentage;
    printf("Enter the first subject:\n");
    scanf("%f",m1);
    printf("Enter the second subject":\n);
    scanf("%f",m2);
    printf("Enter the third subject":\n);
    scanf("%f",m3);
    printf("Enter the fourth subject":\n);
    scanf("%f,m4);
    printf("Enter the fifth subject":,\n);
    scanf("%f,m5);
    total=m1+m2+m3+m4+m5;
    printf("Total of five subjects=%f\n",total);
    average=(m1+m2+m3+m4+m5)/5;
    printf("Average of five subjects=%f\n",average);
    percentage=m1+m2+m3+m4+m5)/500)*100;
    printf("Percentage of five subjects=%f\n",percentage);
    return 0;
}    
